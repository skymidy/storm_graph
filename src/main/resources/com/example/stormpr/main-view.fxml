<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="600.0" minWidth="900.0" prefHeight="600.0" prefWidth="900.0" styleClass="root" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stormpr.MenubarController">
    <ToolBar id="menu_bar" prefHeight="40.0" styleClass="menu-bar">
        <Button mnemonicParsing="false" onMouseClicked="#onCreateNode" prefHeight="24.0" prefWidth="93.0" styleClass="menu-button" text="Create Node" textAlignment="CENTER">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </ToolBar>
   <ScrollPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <content>
      
                                  <AnchorPane id="node_canvas" fx:id="node_canvas" minHeight="600.0" minWidth="900.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="10000.0" prefWidth="10000.0" styleClass="node-canvas">
              <Group id="lines_group" />
          </AnchorPane>
      </content>
   </ScrollPane>
</VBox>
